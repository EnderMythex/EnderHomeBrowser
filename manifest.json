{
  "manifest_version": 3,
  "name": "EnderHomeBrowser",
  "description": "A custom extension that transforms your new tab into a modern dashboard with useful widgets, system statistics and shortcuts.",
  "version": "1.0",
  "author": "EnderMythex",
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "history",
    "system.cpu",
    "system.memory",
    "system.storage",
    "geolocation",
    "clipboardRead",
    "clipboardWrite"
  ],
  "offline_enabled": true,
  "key": "appakaomldonooobekdadejdhmlbbelm",
  "version_name": "1.0.0",
  "minimum_chrome_version": "93",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "incognito": "spanning",
  "homepage_url": "https://github.com/EnderMythex/EnderHomeBrowser",
  "background": {
    "service_worker": "background.js"
  },
  "short_name": "EHB",
  "action": {
    "default_icon": {
      "16": "assets/logo.png",
      "32": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    },
    "default_title": "EnderHomeBrowser"
  }
}
