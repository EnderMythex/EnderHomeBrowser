{
  "manifest_version": 3,
  "name": "EnderHomeBrowser",
  "description": "EnderHomeBrowser made by EnderMythex",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "history",
    "system.cpu",
    "system.memory",
    "system.storage",
    "geolocation"
  ],
  "offline_enabled": true,
  "key": "appakaomldonooobekdadejdhmlbbelm",
  "version_name": "1.0.0",
  "minimum_chrome_version": "93",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "incognito": "spanning",
  "homepage_url": "https://github.com/EnderMythex/EnderHomeBrowser",
  "background": {
    "service_worker": "background.js"
  }
}
